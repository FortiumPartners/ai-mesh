{
  "dashboard": {
    "id": "database-performance",
    "title": "Database Performance - PostgreSQL Monitoring",
    "description": "Comprehensive database performance dashboard with query analysis, connection pooling, and tenant-specific usage patterns",
    "tags": ["database", "postgresql", "performance", "queries", "connections"],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "schemaVersion": 1,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "Database Connection Status",
        "type": "stat",
        "targets": [
          {
            "expr": "pg_up{service_name=\"fortium-monitoring-service\"}",
            "legendFormat": "DB Status",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "color": "red",
                    "text": "DOWN"
                  },
                  "1": {
                    "color": "green",
                    "text": "CONNECTED"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Connection Pool Utilization",
        "type": "graph",
        "targets": [
          {
            "expr": "prisma_pool_connections_active{service_name=\"fortium-monitoring-service\"}",
            "legendFormat": "Active Connections",
            "refId": "A"
          },
          {
            "expr": "prisma_pool_connections_idle{service_name=\"fortium-monitoring-service\"}",
            "legendFormat": "Idle Connections",
            "refId": "B"
          },
          {
            "expr": "prisma_pool_connections_total{service_name=\"fortium-monitoring-service\"}",
            "legendFormat": "Total Pool Size",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Connection Count",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 9,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Connection Pool Health",
        "type": "stat",
        "targets": [
          {
            "expr": "prisma_pool_connections_active{service_name=\"fortium-monitoring-service\"} / prisma_pool_connections_total{service_name=\"fortium-monitoring-service\"} * 100",
            "legendFormat": "Pool Utilization %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "red",
                  "value": 90
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 9,
          "x": 15,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Query Execution Time Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(prisma_query_duration_milliseconds_bucket{service_name=\"fortium-monitoring-service\"}[5m])) by (le))",
            "legendFormat": "P50 Query Time",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(prisma_query_duration_milliseconds_bucket{service_name=\"fortium-monitoring-service\"}[5m])) by (le))",
            "legendFormat": "P95 Query Time",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(prisma_query_duration_milliseconds_bucket{service_name=\"fortium-monitoring-service\"}[5m])) by (le))",
            "legendFormat": "P99 Query Time",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Query time (ms)",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Slow Query Detection",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(prisma_query_duration_milliseconds_bucket{service_name=\"fortium-monitoring-service\",le=\"+Inf\"}[5m])) - sum(rate(prisma_query_duration_milliseconds_bucket{service_name=\"fortium-monitoring-service\",le=\"1000\"}[5m]))",
            "legendFormat": "Slow Queries (>1s)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 0.1
                },
                {
                  "color": "red",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "Database Operations by Type",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(prisma_queries_total{service_name=\"fortium-monitoring-service\",operation=\"findMany\"}[5m]))",
            "legendFormat": "SELECT Operations",
            "refId": "A"
          },
          {
            "expr": "sum(rate(prisma_queries_total{service_name=\"fortium-monitoring-service\",operation=\"create\"}[5m]))",
            "legendFormat": "INSERT Operations",
            "refId": "B"
          },
          {
            "expr": "sum(rate(prisma_queries_total{service_name=\"fortium-monitoring-service\",operation=\"update\"}[5m]))",
            "legendFormat": "UPDATE Operations",
            "refId": "C"
          },
          {
            "expr": "sum(rate(prisma_queries_total{service_name=\"fortium-monitoring-service\",operation=\"delete\"}[5m]))",
            "legendFormat": "DELETE Operations",
            "refId": "D"
          }
        ],
        "yAxes": [
          {
            "label": "Operations per second",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "Query Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(prisma_query_errors_total{service_name=\"fortium-monitoring-service\"}[5m]))",
            "legendFormat": "Query Errors/sec",
            "refId": "A"
          },
          {
            "expr": "sum(rate(prisma_query_errors_total{service_name=\"fortium-monitoring-service\"}[5m])) / sum(rate(prisma_queries_total{service_name=\"fortium-monitoring-service\"}[5m])) * 100",
            "legendFormat": "Query Error Rate %",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Error rate",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 8,
        "title": "Top Slow Queries by Table",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, avg_over_time(prisma_query_duration_milliseconds{service_name=\"fortium-monitoring-service\"}[5m])) by (table, operation)",
            "legendFormat": "{{table}} {{operation}}",
            "refId": "A",
            "format": "table"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "table": "Table",
                "operation": "Operation",
                "Value": "Avg Duration (ms)"
              }
            }
          }
        ],
        "fieldConfig": {
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Avg Duration (ms)"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "ms"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {
                        "color": "green",
                        "value": 0
                      },
                      {
                        "color": "yellow",
                        "value": 100
                      },
                      {
                        "color": "red",
                        "value": 1000
                      }
                    ]
                  }
                }
              ]
            }
          ]
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 9,
        "title": "Connection Acquisition Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(prisma_connection_acquisition_duration_milliseconds_bucket{service_name=\"fortium-monitoring-service\"}[5m])) by (le))",
            "legendFormat": "P95 Connection Acquisition Time",
            "refId": "A"
          },
          {
            "expr": "prisma_pool_connections_waiting{service_name=\"fortium-monitoring-service\"}",
            "legendFormat": "Waiting for Connection",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Time (ms) / Count",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      },
      {
        "id": 10,
        "title": "Tenant-Specific Database Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(prisma_queries_total{service_name=\"fortium-monitoring-service\"}[5m])) by (tenant_id)",
            "legendFormat": "Tenant {{tenant_id}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Queries per second",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 11,
        "title": "Database Resource Consumption by Tenant",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(prisma_query_duration_milliseconds{service_name=\"fortium-monitoring-service\"}) by (tenant_id)",
            "legendFormat": "Tenant {{tenant_id}} CPU Time",
            "refId": "A"
          },
          {
            "expr": "sum(prisma_rows_affected_total{service_name=\"fortium-monitoring-service\"}) by (tenant_id)",
            "legendFormat": "Tenant {{tenant_id}} Rows Affected",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Resource usage",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 32
        }
      },
      {
        "id": 12,
        "title": "Query Optimization Recommendations",
        "type": "logs",
        "targets": [
          {
            "expr": "prisma_query_duration_milliseconds{service_name=\"fortium-monitoring-service\"} > 1000",
            "legendFormat": "Slow Query Alert",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 40
        }
      },
      {
        "id": 13,
        "title": "Database Transaction Patterns",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(prisma_transactions_total{service_name=\"fortium-monitoring-service\"}[5m]))",
            "legendFormat": "Transactions/sec",
            "refId": "A"
          },
          {
            "expr": "sum(rate(prisma_transaction_rollbacks_total{service_name=\"fortium-monitoring-service\"}[5m]))",
            "legendFormat": "Rollbacks/sec",
            "refId": "B"
          },
          {
            "expr": "sum(rate(prisma_transaction_rollbacks_total{service_name=\"fortium-monitoring-service\"}[5m])) / sum(rate(prisma_transactions_total{service_name=\"fortium-monitoring-service\"}[5m])) * 100",
            "legendFormat": "Rollback Rate %",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Rate / Percentage",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 48
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "tenant",
          "type": "query",
          "query": "label_values(prisma_queries_total{service_name=\"fortium-monitoring-service\"}, tenant_id)",
          "current": {
            "value": "All",
            "text": "All"
          },
          "includeAll": true
        },
        {
          "name": "table",
          "type": "query",
          "query": "label_values(prisma_queries_total{service_name=\"fortium-monitoring-service\"}, table)",
          "current": {
            "value": "All",
            "text": "All"
          },
          "includeAll": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Slow Queries",
          "datasource": "SignOz",
          "expr": "prisma_query_duration_milliseconds{service_name=\"fortium-monitoring-service\"} > 1000",
          "iconColor": "orange",
          "textFormat": "Slow query detected: {{table}}.{{operation}}"
        },
        {
          "name": "Connection Pool Full",
          "datasource": "SignOz",
          "expr": "prisma_pool_connections_active{service_name=\"fortium-monitoring-service\"} / prisma_pool_connections_total{service_name=\"fortium-monitoring-service\"} > 0.9",
          "iconColor": "red",
          "textFormat": "Connection pool >90% full"
        }
      ]
    }
  }
}